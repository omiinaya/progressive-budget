{
  "name": "progressive-budget-tracker",
  "version": "2.0.0",
  "description": "A Progressive Web App for tracking budget with offline capabilities and real-time synchronization",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
